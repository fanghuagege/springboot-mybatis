CREATE TABLE `jlg_user` (
  `id` bigint(20) NOT NULL,
  `username` varchar(32) NOT NULL DEFAULT '' COMMENT '名字',
  `pwd` varchar(32) NOT NULL DEFAULT '' COMMENT '密码',
  `nickname` varchar(32) DEFAULT '' COMMENT '昵称',
  `updatetime` datetime DEFAULT NULL COMMENT '更新时间',
  `userclass` varchar(1) DEFAULT NULL COMMENT '用户分类',
  `registertime` datetime DEFAULT NULL COMMENT '注册时间',
  `from` varchar(20) DEFAULT '' COMMENT '来源',
  `channel` varchar(20) DEFAULT '' COMMENT '渠道',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;